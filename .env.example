# Example environment variables
# Copy this file to .env.local and set real values

# Resend API key for sending email notifications (optional)
RESEND_API_KEY=

# Google Analytics Measurement ID (GA4), e.g. G-XXXXXXXXXX
GA_MEASUREMENT_ID=

# The "from" email address used by Resend (optional)
RESEND_FROM=you@example.com

# The destination email to receive contact notifications
CONTACT_RECEIVER_EMAIL=you@example.com
